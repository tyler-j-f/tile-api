<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
        "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
    <!--Ignore test files, we're not going to worry about style errors/warnings from tests. -->
    <suppress files="[a-zA-Z0-9]Test.java"
      checks="[a-zA-Z0-9]*"
    />
    <!--Ignore check, CustomImportOrderCheck , it does not work with the auto-formatter plugin. -->
    <suppress
      checks="CustomImportOrderCheck"
      files="[a-zA-Z0-9]*"
    />
    <suppress
      checks="MemberNameCheck"
      files="TokenConfig.java"
    />
    <suppress
      checks="MemberNameCheck"
      files="ContractConfig.java"
    />
    <suppress
      checks="MemberNameCheck"
      files="SalesConfig.java"
    />
</suppressions>